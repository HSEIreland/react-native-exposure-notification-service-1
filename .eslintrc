{
  "root": true,
  "extends": ["@react-native-community"],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "rules": {
    "comma-dangle": 0,
    "react-hooks/exhaustive-deps": "warn",
    "@typescript-eslint/no-unused-vars": [
      "error",
      {"ignoreRestSiblings": true, "argsIgnorePattern": "^_"}
    ]
  }
}
